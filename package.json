{
  "name": "mamboui",
  "version": "0.0.7",
  "author": "Alejandro Scotti, Scotti Corp, Open Source Community",
  "license": "Apache 2.0",
  "description": "Mambo UI Development Web App",
  "main": "server-dev.cjs",
  "type": "module",
  "scripts": {
    "validate": "npm-run-all --parallel eslint hlint clint",
    "dev": "node server-dev.cjs",
    "start": "node server-prod.cjs",
    "eslint": "npx eslint \"./**/*.js\"",
    "hlint": "npx htmlhint \"./**/*.html\" -i .htmlhintignore",
    "clint": "npx stylelint \"./**/*.css\"",
    "precommit": "npm run validate",
    "prepush": "npm run validate",
    "build": "node ./setup/esbuild.config.cjs buildLib",
    "build-documentation": "npm run build && node ./setup/build_documentation.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "express": "^4.21.2",
    "prettier": "^2.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "babel-eslint": "^10.1.0",
    "esbuild": "^0.14.32",
    "eslint": "^9.16.0",
    "git-cliff": "^2.7.0",
    "globals": "^15.13.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-empty": "^0.1.2",
    "gulp-header-comment": "^0.10.0",
    "gulp-ignore": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-strip-css-comments": "^2.0.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0"
  }
}
